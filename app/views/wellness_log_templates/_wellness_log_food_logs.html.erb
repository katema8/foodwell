<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Food Logs

    <a data-toggle="collapse" href="#wellness_log_food_logs_new_form">
      Add food log
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @wellness_log.food_logs.each do |food_log| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <!-- Show link start -->
        <a href="/food_logs/<%= food_log.id %>">
          <%= food_log.food_item_id %>
        </a>
        <!-- Show link end -->

        <!-- Delete link start -->
        <a href="/delete_food_log_from_wellness_log/<%= food_log.id %>">
          Delete
        </a>
        <!-- Delete link end -->

      </li>
    <% end %>

    <div class="card-footer">
      <div class="collapse" id="wellness_log_food_logs_new_form">
        <!-- New associationcollection form start -->

        <!-- New associationcollection form end -->
      </div>
    </div>

  </ul>
</div>
