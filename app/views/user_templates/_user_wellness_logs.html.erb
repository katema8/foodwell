<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Wellness Logs

    <a data-toggle="collapse" href="#user_wellness_logs_new_form">
      Add wellness log
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.wellness_logs.each do |wellness_log| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <!-- Show link start -->
        <a href="/wellness_logs/<%= wellness_log.id %>">
          <%= wellness_log.gut_wellness %>
        </a>
        <!-- Show link end -->

      </li>
    <% end %>

  </ul>
</div>
